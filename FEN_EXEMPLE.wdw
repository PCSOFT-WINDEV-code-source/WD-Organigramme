        type : 160
      -
        code : |1-
         
         // Si on clique en dehors des éléments de l'organigramme
         si ORGA_ORGANIGRAMME.InfoXY(SourisPosX, sourisposy) = -1 ALORS
         	
         	bDéjàSélectionné est un booléen = (gOrgaElement <> null)	
         
         	si bDéjàSélectionné alors
         		
         		AnimationPrépare(mafenetre)
         		
         		// Dé-sélectionne
         		ORGA_ORGANIGRAMME.SelectMoins(ORGA_ORGANIGRAMME)
         		
         		// Annule la référence
         		gOrgaElement = null
         		GR_SELECTION..Etat = grise
         		animationjoue(animfondu, 15)
         	fin
         FIN
        type : 66050
      -
