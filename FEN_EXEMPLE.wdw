          controls :
           -
             name : BTN_RUB_NOUVEAU_ORGELEMENT
             identifier : 0x2ec27271d3704943
             internal_properties : 
             properties :
              x : 1
              y : 1
              width : 66
              height : 70
             type : 4
             code_elements :
              type_code : 4
              p_codes :
               -
                 code : |1-
                  
                  nID est un entier = -1
                  
                  // Crée une nouvelle fiche
                  si ouvre(FEN_FICHE, nID, (gOrgaElement = null ? 0 sinon gOrgaElement.ID)) ALORS
                  	
                  	// Affiche l'organigramme
                  	HExécuteRequête(REQ_Organigramme)
                  	ORGA_ORGANIGRAMME.Affiche()	
                  	
                  	// Sélectionne la fiche nouvellement créée
                  	pour i = 1 _a_ ORGA_ORGANIGRAMME..Occurrence
                  		si ORGA_ORGANIGRAMME[i].ID = nID ALORS
                  			ORGA_ORGANIGRAMME.SelectPlus(i)
                  			ORGA_ORGANIGRAMME.ExécuteTraitement(trtSélection)
                  			sortir	
                  		FIN
                  	FIN
                  FIN
                 type : 18
               -
                 type : 161
                 enabled : false
           -
             name : BTN_RUB_EDIT_ORGELEMENT
             identifier : 0x2ec2728cd51db2ed
             internal_properties : 
             properties :
              x : 68
              y : 1
              width : 66
              height : 70
             type : 4
             code_elements :
              type_code : 4
              p_codes :
               -
                 code : |1-
                  
                  nID est un entier = gOrgaElement.ID
                  
                  // Modifie la fiche
                  SI Ouvre(FEN_FICHE, nID) ALORS
                  	
                  	// Affiche l'organigramme
                  	HExécuteRequête(REQ_Organigramme)
                  	ORGA_ORGANIGRAMME.Affiche()	
                  	
                  	// Sélectionne la fiche nouvellement créée
                  	POUR i = 1 _a_ ORGA_ORGANIGRAMME..Occurrence
                  		SI ORGA_ORGANIGRAMME[i].ID = nID ALORS
                  			ORGA_ORGANIGRAMME.SelectPlus(i)
                  			ORGA_ORGANIGRAMME.ExécuteTraitement(trtSélection)
                  			SORTIR	
                  		FIN
                  	FIN
                  FIN
                 type : 18
               -
                 type : 161
                 enabled : false
           -
             name : BTN_RUB_SUPPR_ORGELEMENT
             identifier : 0x2ec272e5ee6d0ef1
             internal_properties : 
             properties :
              x : 135
              y : 1
              width : 66
              height : 70
             type : 4
             code_elements :
              type_code : 4
              p_codes :
               -
                 code : |1-
                  
                  nID 			est un entier = gOrgaElement.ID
                  nIDResponsable	est un entier
                  
                  // Recherche la fiche
                  HLitRecherchePremier(Personne, IDPersonne, nID)
                  si pas htrouve(Personne) alors erreur("Suppression impossible")
                  
                  //1 : &Supprimer
                  //2 : &Ne pas supprimer
                  si Dialogue(<§?1EC28C090073439A§>, gOrgaElement.Titre) = 1 alors
                  
                  	// Pas de responsable
                  	si personne.IDResponsable = 0 ALORS
                  		
                  		// On supprime sans vérification (la suppression attribuera un responsable par défaut aux subordonnés (ie : aucun))
                  		hsupprime(Personne)
                  	sinon
                  		
                  		// On remonte la hiérarchie
                  		HLitRecherchePremier(Personne, IDPersonne, personne.IDResponsable)
                  		si htrouve(personne) ALORS
                  			
                  			// Pas de responsable supérieur
                  			si personne.IDResponsable = 0 ALORS
                  				
                  				// On supprime sans vérification (la suppression attribuera un responsable par défaut aux subordonnés (ie : aucun))
                  				HLitRecherchePremier(Personne, IDPersonne, nID)
                  				hsupprime(personne)
                  			sinon
                  		
                  				// Conserve l'identifiant du responsable supérieur
                  				nIDResponsable = personne.IDResponsable			
                  
                  				// On doit modifier le responsable des subordonnés
                  				pour toute personne avec IDResponsable = nID
                  					personne.IDResponsable = nIDResponsable
                  					hmodifie(Personne)
                  				FIN
                  				
                  				// On re-sélectionne la personne à supprimer
                  				HLitRecherchePremier(Personne, IDPersonne, nID)
                  				HSupprime(Personne)
                  			FIN
                  		FIN
                  	FIN
                  	
                  	// Affiche l'organigramme
                  	HExécuteRequête(REQ_Organigramme)
                  	ORGA_ORGANIGRAMME.Affiche()	
                  	
                  	AnimationPrépare(MaFenêtre)
                  	
                  	// Annule la référence
                  	gOrgaElement = Null
                  	GR_SELECTION..Etat = Grisé
                  	AnimationJoue(animFondu, 15)
                  fin
                 type : 18
               -
                 type : 161
                 enabled : false
