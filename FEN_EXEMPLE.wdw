     code : |1-
      
      global
      	
      	// Elément sélectionné
      	gOrgaElement	est un OrgaElément dynamique
      
      
      local
      	
      	sOrganigramme est une chaine
      	nOrganigramme est un entier
      
      	si Organigrammes..Occurrence > 0 alors
      
      		// Crée les options de menu permettant d'ouvrir les organigrammes
      		pour tout sOrganigramme, nOrganigramme de Organigrammes
      			MenuAjouteOption(MENU_OUVRIR, "OPT_ORGA" + nOrganigramme, ExtraitChaîne(sOrganigramme, rangDernier, fSép), OuvreOrganigramme, sOrganigramme)
      			ExécuteCode("OPT_ORGA" + nOrganigramme + "..Image = ""orga_icon.png""")
      		FIN
      		
      		MENU_OUVRIR.OPT_PLACEHOLDER..Visible = faux
      	sinon
      		MENU_OUVRIR.OPT_PLACEHOLDER..Visible = vrai
      	fin
      	
      	// Ouvre l'exemple
      	si Organigrammes[1] = frepexe + ["\"] + "Exemple" ALORS
      		OuvreOrganigramme(Organigrammes[1])
      	FIN
