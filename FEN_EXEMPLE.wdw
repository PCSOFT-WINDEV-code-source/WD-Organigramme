  procedures :
   -
     name : OuvreOrganigramme
     internal_properties : CAAAAAgAAAAM34km+p2LM0Jafr/gijXeTImHicFj7lFu9FrqtgEK91qgYssHbROYrXPWPQuCRnMMZ684xbe6oq1nscH9xKapdVr2FWTPC1MVk/+hO52w5+vn7IjZ6fSvrcAYUpDA0PhZ/QJLYO5TzRKT4VFwJvmoQ216mG+7u8SECyh/lCtC1H7uDeo=
     procedure_id : 2216461707538929093
     type_code : 14
     code : |1-
      // Résumé : Ouvre un organigramme
      // Syntaxe :
      //[ <Résultat> = ] OuvreOrganigramme (<sOrganigramme> est chaîne ANSI)
      //
      // Paramètres :
      //	sOrganigramme (chaîne ANSI) : Chemin de l'organigramme
      // Valeur de retour :
      // 	booléen : Vrai si l'organigramme a été ouvert, Faux sinon
      //
      // Traitement automatique des erreurs : exécuter le bloc de code CAS ERREUR
      //
      PROCEDURE OuvreOrganigramme(local sOrganigramme est une chaine)
      	
      	// Vide l'organigramme
      	ORGA_ORGANIGRAMME.SupprimeTout()
      	
      	// Localisation des données
      	hchangerep(Personne, sOrganigramme)
      	HCréationSiInexistant(Personne)
      	
      	// Affiche l'organigramme
      	HExécuteRequête(REQ_Organigramme)
      		
      	AnimationPrépare(mafenetre)
      	
      	MaFenêtre..Titre = "WD Organigramme [" + ExtraitChaîne(sOrganigramme, rangdernier, fsep) + "]"
      	ORGA_ORGANIGRAMME.Affiche()
      
      	// Active la création
      	BTN_RUB_NOUVEAU_ORGELEMENT..etat = actif
      	
      	gOrgaElement = null
      	GR_SELECTION..Etat = grise
      
      	animationjoue(animFondu, 15)
      
      renvoyer vrai
      
      CAS ERREUR:
      	renvoyer faux
