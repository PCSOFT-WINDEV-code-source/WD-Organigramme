      -
        code : |1+
         // Procédure appelée pour chaque dessin d'un élément d'organigramme
         // <gOrgaElement> : l'élément à afficher 
         PROCEDURE FI_WINDEVVueOrgaElement( gOrgaElement est un OrgaElément )
         
         // Initialisation des libellés à partir de l'objet à afficher
         LIB_TitreOrgaElement     = gOrgaElement.Titre
         LIB_SousTitreOrgaElement = gOrgaElement.Contenu
         
         //si on a une image
         
         iImage est une image = gOrgaElement.Image
         si iImage..valide alors
         	// on rend l'image visible et on l'affecte au champ image
         	IMG_ImageOrgaElement..Visible = vrai
         	IMG_ImageOrgaElement 	 = gOrgaElement.Image
         sinon
         	IMG_ImageOrgaElement..visible = faux
         	nDecalX est entier = IMG_ImageOrgaElement..Largeur + 6 // marge	
         	// on récupère la place pour le libellé
         	LIB_TitreOrgaElement..X			  = LIB_TitreOrgaElement..XInitial- nDecalX
         	LIB_TitreOrgaElement..Largeur 	  = LIB_TitreOrgaElement..Largeur+ nDecalX
         	LIB_SousTitreOrgaElement..X		  = LIB_SousTitreOrgaElement..XInitial- nDecalX
         	LIB_SousTitreOrgaElement..Largeur = LIB_SousTitreOrgaElement..Largeur + nDecalX
         FIn
         
         
      -
        type : 15
